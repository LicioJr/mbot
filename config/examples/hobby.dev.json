{
    "db": {
        "adapters": {
            "adap-disk": "sails-disk"
        },
        "connections": {
            "hobby": {
                "adapter": "adap-disk",
                "filePath": ".tmp/",
                "inMemoryOnly": true
            }
        },
        "defaults": {
            "migrate": "safe",
            "autoCreatedAt": true,
            "autoUpdatedAt": true
        }
    }
}